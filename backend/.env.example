# Environment
NODE_ENV=development
PORT=3000

# MongoDB
MONGODB_URI=mongodb://localhost:27017/iastam-checkin

# CORS
CORS_ORIGIN=http://localhost:3000,http://localhost:3001

# Rate Limiting
THROTTLE_TTL=60000
THROTTLE_LIMIT=100

# File Upload
MAX_FILE_SIZE=10485760
UPLOAD_DEST=./uploads

# API
API_VERSION=1
API_PREFIX=api
# Check-in Settings
CHECKIN_LATE_THRESHOLD_MINUTES=10